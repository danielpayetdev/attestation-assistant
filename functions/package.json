    "@types/puppeteer": "^2.0.1",
    "@types/uuid-apikey": "^1.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "firebase-functions-test": "^0.1.6",
    "ts-loader": "^6.2.2",
    "tslint": "^5.12.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
